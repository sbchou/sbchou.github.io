{
  "title": "Person",
  "type": "object",
  "id": "person",
  "properties": {
    "name": {
      "type": "string",
      "description": "First and Last name",
      "minLength": 4
    },
    "email": {
      "type": "string",
      "description": "Primary Email",
      "minLength": 8
    },
    "age": {
      "type": "integer",
      "default": 21,
      "minimum": 18,
      "maximum": 99
    },
    "gender": {
      "type": "string",
      "enum": [
        "male",
        "female",
      ]
    }
  }
}
